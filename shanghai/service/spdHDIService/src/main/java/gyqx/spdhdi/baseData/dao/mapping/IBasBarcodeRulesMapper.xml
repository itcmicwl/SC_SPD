	<select id="list"  resultMap="basBarcodeRulesMap" parameterType="gyqx.spdhdi.po.BasBarcodeRules">
		<![CDATA[
			SELECT 
			    id,
			    barcode,
			    barcode_len,
			    barleader,
			    consumables_position,
			    consumables_len,
			    is_main,
			    period_position,
